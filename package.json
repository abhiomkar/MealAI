{
  "name": "meal-ai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vercel env pull .env.local && prisma generate && next dev",
    "build": "prisma generate && dotenv -e .env.local prisma db push && next build",
    "start": "npm run build && next start",
    "lint": "next lint",
    "dbPush": "dotenv -e .env.local prisma db push",
    "dbStudio": "dotenv -e .env.local prisma studio",
    "env": "vercel env pull .env.local"
  },
  "dependencies": {
    "@clerk/clerk-js": "^4.47.2",
    "@clerk/nextjs": "^4.21.1",
    "@clerk/themes": "^1.7.5",
    "@prisma/client": "^4.15.0",
    "@types/node": "20.3.0",
    "@types/react": "18.2.11",
    "@types/react-dom": "18.2.4",
    "@vercel/postgres": "^0.3.1",
    "autoprefixer": "10.4.14",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.5",
    "next": "13.4.5",
    "postcss": "8.4.24",
    "prisma": "^4.15.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.3"
  },
  "devDependencies": {
    "dotenv-cli": "^7.2.1"
  }
}
